    "version": "0.2.0",
    "configurations": [
        {
            "type": "rdbg",
            "name": "Debug specific file with rdbg",
            "request": "launch",
            "script": "${file}",
            "args":["-f IPAccessControlList", "-v true"],
            "askParameters": true
        },
        {
            "type": "rdbg",
            "name": "Debug current file with rdbg",
            "request": "launch",
            "script": "${file}",
            "args":["-v true"],
            "askParameters": true
        },
        {
            "type": "rdbg",
            "name": "Attach with rdbg",
            "request": "attach"
        }
    ]
}