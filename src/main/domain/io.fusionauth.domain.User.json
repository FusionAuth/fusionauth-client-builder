{
  "packageName" : "io.fusionauth.domain",
  "type" : "User",
  "description" : "/**\n * The global view of a User. This object contains all global information about the user including birth date, registration information\n * preferred languages, global attributes, etc.\n *\n * @author Seth Musselman\n */\n",
  "extends" : [ {
    "type" : "SecureIdentity"
  } ],
  "implements" : [ {
    "type" : "Buildable",
    "typeArguments" : [ {
      "type" : "User"
    } ]
  }, {
    "type" : "_InternalJSONColumn"
  }, {
    "type" : "Tenantable"
  } ],
  "fields" : {
    "preferredLanguages" : {
      "type" : "List",
      "typeArguments" : [ {
        "type" : "Locale"
      } ]
    },
    "active" : {
      "type" : "boolean"
    },
    "birthDate" : {
      "type" : "LocalDate"
    },
    "cleanSpeakId" : {
      "type" : "UUID"
    },
    "data" : {
      "type" : "Map",
      "typeArguments" : [ {
        "type" : "String"
      }, {
        "type" : "Object"
      } ]
    },
    "email" : {
      "type" : "String"
    },
    "expiry" : {
      "type" : "ZonedDateTime"
    },
    "firstName" : {
      "type" : "String"
    },
    "fullName" : {
      "type" : "String"
    },
    "imageUrl" : {
      "type" : "URI"
    },
    "insertInstant" : {
      "type" : "ZonedDateTime"
    },
    "lastName" : {
      "type" : "String"
    },
    "lastUpdateInstant" : {
      "type" : "ZonedDateTime"
    },
    "middleName" : {
      "type" : "String"
    },
    "mobilePhone" : {
      "type" : "String"
    },
    "parentEmail" : {
      "type" : "String"
    },
    "tenantId" : {
      "type" : "UUID"
    },
    "timezone" : {
      "type" : "ZoneId"
    },
    "memberships" : {
      "type" : "List",
      "typeArguments" : [ {
        "type" : "GroupMember"
      } ]
    },
    "registrations" : {
      "type" : "List",
      "typeArguments" : [ {
        "type" : "UserRegistration"
      } ]
    }
  }
}